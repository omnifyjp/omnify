#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/f.satoshi/Herd/omnify-ts/packages/mcp-server/dist/node_modules:/Users/f.satoshi/Herd/omnify-ts/packages/mcp-server/node_modules:/Users/f.satoshi/Herd/omnify-ts/packages/node_modules:/Users/f.satoshi/Herd/omnify-ts/node_modules:/Users/f.satoshi/Herd/node_modules:/Users/f.satoshi/node_modules:/Users/node_modules:/node_modules:/Users/f.satoshi/Herd/omnify-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/f.satoshi/Herd/omnify-ts/packages/mcp-server/dist/node_modules:/Users/f.satoshi/Herd/omnify-ts/packages/mcp-server/node_modules:/Users/f.satoshi/Herd/omnify-ts/packages/node_modules:/Users/f.satoshi/Herd/omnify-ts/node_modules:/Users/f.satoshi/Herd/node_modules:/Users/f.satoshi/node_modules:/Users/node_modules:/node_modules:/Users/f.satoshi/Herd/omnify-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@famgia/omnify-mcp/dist/index.js" "$@"
else
  exec node  "$basedir/../@famgia/omnify-mcp/dist/index.js" "$@"
fi
